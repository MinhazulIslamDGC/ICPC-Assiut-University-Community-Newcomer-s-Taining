// H Two Numbers

# include <iostream>
# include <cmath>
using namespace std;

int main()
{
    float A, B; // While using the ceil/floor/round operators, data type of the numbers must be float/double/long double
    cin >> A >> B;
    float div;  // Same reason
    div = A/B;
    
    cout << "floor " << A << " / " << B << " = " << floor(div) << '\n';
    
    cout << "ceil " << A << " / " << B << " = " << ceil(div) << '\n';
    
    cout << "round " << A << " / " << B << " = " << round(div) << '\n';
}
